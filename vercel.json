{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "status": 302,
      "headers": {
        "Location": "/api-docs"
      }
    },
    {
      "src": "/api-docs",
      "dest": "/src/index.js"
    },
    {
      "src": "/api/.*",
      "dest": "/src/index.js"
    }
  ]
}