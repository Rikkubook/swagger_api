{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/swagger.json",
      "dest": "/index.js"
    },
    {
      "src": "/api-docs",
      "dest": "/index.js"
    },
    {
      "src": "/api/.*",
      "dest": "/index.js"
    }
  ]
}
